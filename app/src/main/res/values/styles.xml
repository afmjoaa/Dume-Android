<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/colorNarvik</item>
        <item name="colorControlHighlight">@color/colorDarkAzure</item>
    </style>

    <style name="MyDarkBlueButton" parent="Widget.AppCompat.Button.Colored">
        <item name="colorButtonNormal">@color/colorDarkBlue</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

</resources>
